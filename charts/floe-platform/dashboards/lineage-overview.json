{
  "uid": "floe-lineage-overview",
  "title": "Floe Data Lineage Overview",
  "tags": ["floe", "openlineage", "lineage"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-3h",
    "to": "now"
  },
  "editable": true,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Lineage Events",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(openlineage_event_total[1h]))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "blue"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "START Events",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(openlineage_event_total{event_type=\"START\"}[1h]))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "COMPLETE Events",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(openlineage_event_total{event_type=\"COMPLETE\"}[1h]))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "blue"
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "FAIL Events",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(openlineage_event_total{event_type=\"FAIL\"}[1h]))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Lineage Event Rate",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "rate(openlineage_event_total[5m])",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "lastNotNull"]
        }
      }
    },
    {
      "id": 6,
      "type": "piechart",
      "title": "Event Type Distribution",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by(event_type) (increase(openlineage_event_total[1h]))",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "pie"
      }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Unique Jobs Tracked",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "count(count by(job_name) (openlineage_event_total))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "purple"
          }
        }
      }
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Unique Datasets",
      "gridPos": {
        "x": 6,
        "y": 12,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "count(count by(dataset_name) (openlineage_dataset_facet_total))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "orange"
          }
        }
      }
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Emission Points",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "count(count by(producer) (openlineage_event_total))",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      }
    },
    {
      "id": 10,
      "type": "table",
      "title": "Top Jobs by Event Count",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "topk(10, sum by(job_name) (increase(openlineage_event_total[1h])))",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "renameByName": {
              "job_name": "Job Name",
              "Value": "Event Count"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Event Count",
            "desc": true
          }
        ]
      }
    },
    {
      "id": 11,
      "type": "bargauge",
      "title": "Events by Producer",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by(producer) (increase(openlineage_event_total[1h]))",
          "legendFormat": "{{producer}}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Lineage Graph Size (Nodes)",
      "gridPos": {
        "x": 18,
        "y": 12,
        "w": 6,
        "h": 12
      },
      "targets": [
        {
          "expr": "openlineage_graph_node_count",
          "legendFormat": "Nodes",
          "refId": "A"
        },
        {
          "expr": "openlineage_graph_edge_count",
          "legendFormat": "Edges",
          "refId": "B"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    }
  ]
}
